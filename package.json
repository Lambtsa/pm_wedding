{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development node server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "format:fix": "prettier --ignore-path .gitignore --write .",
    "test": "jest --watch --verbose",
    "cypress:open": "cypress open",
    "remote:restart": "bash ./deploy/restart.sh",
    "remote:destroy": "bash ./deploy/delete.sh",
    "remote:start": "bash ./deploy/start.sh",
    "remote:deploy": "bash ./deploy/pull.sh && bash ./deploy/deploy.sh",
    "lint:check": "eslint --ignore-path .gitignore --ext .jsx,.js,.tsx,.ts ./",
    "lint:fix": "npm run lint:check -- --fix",
    "lint:tsc": "tsc --noEmit --project tsconfig.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "2.9.10",
    "@next/font": "13.1.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "mapbox-gl": "2.12.0",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.41.5",
    "react-intl": "6.2.5",
    "styled-components": "5.3.6",
    "typescript": "4.9.4",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@svgr/webpack": "^6.5.1",
    "@types/jest": "^29.2.5",
    "@types/mapbox-gl": "^2.7.10",
    "@types/styled-components": "^5.1.26",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "babel-plugin-styled-components": "^2.0.7",
    "cypress": "^12.3.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.3",
    "url-loader": "^4.1.1",
    "uuid": "9.0.0"
  }
}
